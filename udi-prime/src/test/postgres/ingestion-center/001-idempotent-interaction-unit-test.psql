	WHERE sat_intr.hub_interaction_id = v_interaction_id
	  AND COALESCE(sat_intr.uri, '') = v_interaction_key
	  AND COALESCE(sat_intr.nature, '') IN (
	      'org.techbd.service.http.Interactions$RequestResponseEncountered', 
